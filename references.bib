	eissn = {2008-6822},
	eprint = {https://ijnaa.semnan.ac.ir/article_5839_974f463dd2608e7e1f66880dc8a2dd41.pdf},
	issn = {2008-6822},
	journal = {International Journal of Nonlinear Analysis and Applications},
	keywords = {Fish diseases, Detection, Classification, Convolutional Neural Network (CNN)},
	number = {1},
	pages = {1977-1984},
	publisher = {Semnan University},
	title = {Fish diseases detection using convolutional neural network (CNN)},
	url = {https://ijnaa.semnan.ac.ir/article_5839.html},
